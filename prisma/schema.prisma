generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id                Int      @id @default(autoincrement())
  createdAt         DateTime @default(now())
  githubRepoId      BigInt   @unique
  repoName          String
  railwayProjectId  String
  railwayServiceId  String
}
